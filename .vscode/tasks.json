{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "check mbx",
			"type": "shell",
			"command": "cargo",
			"args": [
				"check",
				"--all-features",
				"--tests"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"options": {
				"env": {}
			},
			"presentation": {
				"clear": true,
				"revealProblems": "onProblem"
			},
			"problemMatcher": [
				{
					"base": "$rustc",
					"fileLocation": [
						"relative"
					]
				}
			]
		},
		{
